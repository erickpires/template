{
    "command": "gcc",
    "args": [
        "-Wall",
        "-o",
        "temp",
        "template.c",
        "string_buffer.c",
        "-g"
        ],
    "isShellCommand": true,
    "showOutput": "always",
    "problemMatcher": {
        "owner": "c",
        "fileLocation": [
            "relative",
            "${workspaceRoot}"
        ],
        "pattern": {
            "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
        }
  }